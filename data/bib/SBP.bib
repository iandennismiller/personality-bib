  issn = {03012212},
  abstract = {None},
  journal = {Social Behavior and Personality},
  keywords = {Emotionally disturbed | Learning disabled | Negative wording | Rosenberg Self-Esteem Scale | Self-concept | Self-esteem measurement | Subjective discomfort},
  
}

@article{{10.2224/sbp.2009.37.10.1369},
  title = {Does equality contribute to prosocial behavior?},
  author = {Liu, Chang Jiang},
  year = {2009-12-01},
  volume = {10},
  issn = {03012212},
  abstract = {It is hypothesized that people who obtain their initial resources in an equal way will behave prosocially since they endorse equality. Using a one-factor (Allocation system: equality, equity, need, vs. individualism) between-participants design, in this study participants were presented with a policy scenario. Results showed that an equality-based policy was rated most satisfactory, whereas the mean of contribution amount under the equality condition was less than that of the average contribution amount under the other three conditions. This finding implies that equality undermines people's prosocial behavior, a finding worthy of further examination. © Society for Personality Research (Inc.).},
  journal = {Social Behavior and Personality},
  keywords = {Equality | Prosocial behavior | Social policy},
  
}

