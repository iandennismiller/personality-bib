}

@article{{None},
  title = {Subjective pseudocyesis in normal women who exhibit enhanced imaginings and elevated indicators of electrical lability within the temporal lobes: Implications for the "missing embryo syndrome"},
  author = {Persinger, M. A.},
  year = {1996-12-01},
  volume = {2},
