}

@article{{None},
  title = {Young children's theories of mind about empathic and selfish motives},
  author = {Ginsburg, Harvey J. and Ogletree, Shirley M. and Silakowski, Tammy D. and Bartels, Roger D. and Burk, Shannon L. and Marc Turner, G.},
